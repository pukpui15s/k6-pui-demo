{
  "name": "tester-demo",
  "version": "1.0.0",
  "description": "Node.js Backend ตัวอย่าง สำหรับทดสอบด้วย K6 - เหมาะสำหรับมือใหม่",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test:smoke": "k6 run k6/smoke-test.js",
    "test:load": "k6 run k6/load-test.js",
    "test:stress": "k6 run k6/stress-test.js",
    "test:spike": "k6 run k6/spike-test.js",
    "test:all": "npm run test:smoke && npm run test:load"
  },
  "keywords": ["k6", "load-testing", "nodejs", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.0"
  }
}
